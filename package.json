{
  "name": "@steipete/oracle",
  "version": "1.1.0",
  "description": "CLI wrapper around OpenAI Responses API with GPT-5 Pro and GPT-5.1 high reasoning modes.",
  "type": "module",
  "main": "dist/bin/oracle-cli.js",
  "bin": {
    "oracle": "dist/bin/oracle-cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "pnpm run build && node ./dist/scripts/run-cli.js",
    "oracle": "pnpm start",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm run typecheck && biome lint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "pnpm run build"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steipete/oracle.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steipete/oracle/issues"
  },
  "homepage": "https://github.com/steipete/oracle#readme",
  "dependencies": {
    "chalk": "^5.6.2",
    "chrome-cookies-secure": "^3.0.0",
    "chrome-launcher": "^1.2.1",
    "chrome-remote-interface": "^0.33.3",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.3",
    "gpt-tokenizer": "^3.4.0",
    "keytar": "^7.9.0",
    "kleur": "^4.1.5",
    "markdansi": "^0.1.1",
    "openai": "^6.9.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "4.0.9",
    "devtools-protocol": "^0.0.1545402",
    "puppeteer-core": "^24.30.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "chrome-cookies-secure",
      "keytar",
      "sqlite3"
    ]
  }
}
